<div class="search_form container">
  <%= form_with url: search_path, method: :get, local: true do |f| %>
  <%= f.text_field :content %>
  <%= f.select :hobby_code, options_for_select({ "スポーツ" => "スポーツ", "プログラミング" => "プログラミング", "ゲーム" => "ゲーム", "料理" => "料理", "食事" => "食事", "読書" => "読書", "勉強" => "勉強", "映画" => "映画", "イベント" => "イベント", "その他" => "その他" }) %>
  <%= f.submit '検索' %>
  <% end %>
</div>